<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/dummycircle">
        <shape android:shape="oval"
          xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/uikit_white" />
            <stroke android:width="@dimen/slider_thumb_stroke_width" />
            <size android:height="@dimen/slider_thumb_height" android:width="@dimen/slider_thumb_width" />
        </shape>
    </item>
    <item android:id="@id/originalcircle">
        <shape android:shape="oval"
          xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/uikit_white" />
            <stroke android:width="@dimen/slider_thumb_stroke_width" />
            <size android:height="@dimen/slider_thumb_height" android:width="@dimen/slider_thumb_width" />
        </shape>
    </item>
</layer-list>