<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="ring" android:innerRadiusRatio="@dimen/uid_progress_bar_circular_middle_innerRadiusRatio" android:thicknessRatio="@dimen/uid_progress_bar_circular_middle_thicknessRatio" android:useLevel="false" />
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="0.0" android:toDegrees="360.0">
            <shape android:shape="ring" android:innerRadiusRatio="@dimen/uid_progress_bar_circular_middle_innerRadiusRatio" android:thicknessRatio="@dimen/uid_progress_bar_circular_middle_thicknessRatio" android:useLevel="false" />
        </rotate>
    </item>
</layer-list>