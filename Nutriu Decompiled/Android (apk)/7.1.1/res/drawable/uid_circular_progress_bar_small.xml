<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:innerRadiusRatio="@dimen/uid_progress_bar_circular_small_innerRadiusRatio" android:thicknessRatio="@dimen/uid_progress_bar_circular_small_thicknessRatio" android:useLevel="false" />
        </rotate>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:innerRadiusRatio="@dimen/uid_progress_bar_circular_small_innerRadiusRatio" android:thicknessRatio="@dimen/uid_progress_bar_circular_small_thicknessRatio" />
        </rotate>
    </item>
</layer-list>